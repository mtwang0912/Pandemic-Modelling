    "# Run each simulation 10 times, find average and standard deviation\n",
    "\n",
    "n_vals = [200, 400, 600, 800, 1000, 1500, 2000, 2500, 3000, 5000, 8000]\n",
    "\n",
    "sw_cons = [2, 4, 6, 8]\n",
    "\n",
    "for c in sw_cons:\n",
    "    results = []\n",
    "    for n in tqdm.tqdm(n_vals):\n",
    "        aspl_list = []\n",
    "        for _ in range(10):\n",
    "            g = nx.watts_strogatz_graph(n = n, k = c, p = 0.2, seed=None)\n",
    "\n",
    "            ASPL_list = []\n",
    "            for C in (g.subgraph(c).copy() for c in nx.connected_components(g)):\n",
    "                ASPL_list.append(nx.average_shortest_path_length(C))\n",
    "            aspl_list.append(statistics.mean(ASPL_list))\n",
    "        results.append([n, c, statistics.mean(aspl_list), statistics.stdev(aspl_list)])\n",
    "    df = pd.DataFrame(results)\n",
    "    df.columns = ['Number of Nodes', 'Connections', 'Mean ASPL', 'StdDev ASPL']\n",
    "    date = datetime.datetime.today()\n",
    "    df.to_csv('aspl_nodes_{}_{}.csv'.format(c, date))"
   ]
