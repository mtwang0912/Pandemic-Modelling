    "nodes = [500, 1000, 2000, 4000]\n",
    "connections = [2, 4, 6, 8, 10, 12, 14]\n",
    "\n",
    "repeats = 10\n",
    "\n",
    "for n in nodes:\n",
    "    df = []\n",
    "    for c in connections:\n",
    "        dummy_list = []\n",
    "        for _ in tqdm.tqdm(range(repeats)):\n",
    "            g = nx.watts_strogatz_graph(n = n, k = c, p = 0.5, seed=None)\n",
    "\n",
    "            edge_list = [(u, v) for (u, v, d) in g.edges(data = True)] # all edges\n",
    "\n",
    "            for j in edge_list:\n",
    "                g.add_edge(j[0], j[1], weight = 1) \n",
    "\n",
    "            ASPL_list = []\n",
    "            for C in (g.subgraph(c).copy() for c in nx.connected_components(g)):\n",
    "                ASPL_list.append(nx.average_shortest_path_length(C, weight= 'weight'))\n",
    "            dummy_list.append(statistics.mean(ASPL_list))\n",
    "\n",
    "        df.append([n, c, statistics.mean(dummy_list), statistics.stdev(dummy_list)])\n",
    "    df_csv = pd.DataFrame(df)\n",
    "    df_csv.columns = ['Number of Nodes', 'Average Number of Connections', 'Mean ASPL', 'Std. Dev ASPL']\n",
    "    date = datetime.datetime.today()\n",
    "    df_csv.to_csv('nodes_con_aspl_{}_{}.csv'.format(n, date))\n",
    "\n",
    "        "
